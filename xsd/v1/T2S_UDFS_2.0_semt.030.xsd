            <xs:element name="QryDef" type="IntraPositionQueryDefinition3">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">QueryDefinition</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Defines the intra-position movement modification query criteria.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
