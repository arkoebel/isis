        <xs:restriction base="xs:string">
            <xs:enumeration value="MAKT">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">MarketSide</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Instruction is for a market side transaction.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="CLNT">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">ClientSide</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Instruction is for a client side transaction.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="MarketIdentification2Choice">
        <xs:annotation>
            <xs:documentation source="Name" xml:lang="EN">MarketIdentification2Choice</xs:documentation>
            <xs:documentation source="Definition" xml:lang="EN">Choice of market identification.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:choice>
                <xs:element name="MktIdrCd" type="MICIdentifier">
                    <xs:annotation>
